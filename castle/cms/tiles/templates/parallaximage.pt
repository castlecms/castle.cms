<tal:container>
  <tal:defs tal:define="images python: view.get_images();">
    <div class="parallax-image-tile-container">
      <tal:container tal:condition="python: images['image_one']">
        <section class="parallax-section parallax parallax-bg1"
                tal:define="img python: images['image_one']"
                style="--bg1-image: url('${img/absolute_url}');">
            <h1>Cute Kitten</h1>
        </section>
      </tal:container>
        <section class="parallax-section parallax-static">
            <h1>Boring</h1>
        </section>
      <tal:container tal:condition="python: images['image_two']">
        <section class="parallax-section parallax parallax-bg2"
                tal:define="img python: images['image_two']"
                style="--bg2-image: url('${img/absolute_url}');">
            <h1>Fluffy Kitten</h1>
        </section>
      </tal:container>
    </div>
  </tal:defs>
</tal:container>