<?xml version="1.0"?>
<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="plone">

  <!-- depends on plone -->

  <!-- castle-components-search -->
  <records prefix="plone.resources/castle-components-search"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
