<?xml version="1.0"?>
<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="plone">

  <record name="plone.request_access_enabled"
          interface="castle.cms.interfaces.controlpanel.ISecuritySchema"
          field="request_access">
    <value>False</value>
  </record>
  <record name="plone.request_access_form_path"
          interface="castle.cms.interfaces.controlpanel.ISecuritySchema"
          field="request_access_form_path">
    <value>/@@request-form</value>
  </record>
  <record name="plone.request_access_form_email_addresses"
          interface="castle.cms.interfaces.controlpanel.ISecuritySchema"
          field="request_access_form_email_addresses">
    <value></value>
  </record>
  <record name="plone.request_access_form_accepted_fields"
          interface="castle.cms.interfaces.controlpanel.ISecuritySchema"
          field="request_access_form_accepted_fields">
    <value>
      name
      email
      phone
    </value>
  </record>

</registry>
